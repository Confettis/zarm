{
  "name": "@zarm-design/icons",
  "version": "0.0.0",
  "description": "",
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf types lib coverage",
    "test": "exit 0",
    "coverage": "rimraf coverage && yarn test --coverage",
    "build:lib": "rimraf types lib && tsc --outDir types -d --emitDeclarationOnly && babel react --extensions .ts,.tsx --out-dir lib --copy-files",
    "build:react": "rimraf react && svgr ./svg --out-dir ./react",
    "build:svg": "rimraf svg && node ./scripts/exportSVG.js",
    "build": "yarn build:svg && yarn build:react && yarn build:lib"
  },
  "author": "jerome.lin@zhongan.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@svgr/cli": "^5.5.0",
    "shelljs": "^0.8.4"
  },
  "dependencies": {
    "zarm": "^2.8.2"
  }
}
